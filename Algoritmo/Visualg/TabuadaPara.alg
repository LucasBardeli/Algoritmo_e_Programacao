algoritmo "Tabuada com Para"

var

   num, i, tabuada: inteiro

inicio

      escreva ("Informe o número que deseja: ")
      leia (num)
      
      para i <- 0 ate 10 faca
           tabuada <- num * i
           
           escreval (num, " x", i, " =", tabuada)
      fimpara
      

fimalgoritmo
