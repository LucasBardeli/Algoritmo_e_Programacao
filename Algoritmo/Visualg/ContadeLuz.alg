algoritmo "Conta de luz"

var
   valor: real

inicio

      escreva ("Informe o valor da sua conta de luz: R$")
      leia (valor)
      
      se (valor > 800.00) entao
         escreval
         escreval ("Você está gastando muito !")
      senao
         escreval
         escreval ("Seu gasto foi normal.")
      fimse
      
fimalgoritmo
