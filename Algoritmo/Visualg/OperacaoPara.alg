algoritmo "Repetição Para"

var

   i, a, acum: inteiro

inicio

      para i <- 1 ate 20 faca
           a <- i * 2
           acum <- acum + a
           
      escreval ("O valor acumulado é: ", acum)
      fimpara
      
      escreval
      escreval ("O valor de A é: ", a)

fimalgoritmo
